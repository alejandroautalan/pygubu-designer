<?xml version='1.0' encoding='utf-8'?>
<interface version="1.5" author="PygubuDesigner 0.41.4">
  <project>
    <settings>
      <setting id="name">Ask Save changes dialog</setting>
      <setting id="description">Dialog with three buttons: Don't Save, Cancel, Save</setting>
      <setting id="module_name">ask_save_changes</setting>
      <setting id="template">codescript</setting>
      <setting id="main_widget">dialog</setting>
      <setting id="main_classname">AskSaveChangesDialog</setting>
      <setting id="main_menu" />
      <setting id="output_dir">../src/pygubudesigner/services</setting>
      <setting id="output_dir2" />
      <setting id="import_tkvariables">False</setting>
      <setting id="use_ttk_styledefinition_file">False</setting>
      <setting id="use_i18n">True</setting>
      <setting id="all_ids_attributes">False</setting>
      <setting id="generate_code_onsave">False</setting>
      <setting id="use_window_centering_code">False</setting>
      <setting id="ttk_style_definition_file" />
    </settings>
    <customwidgets />
    <resources />
  </project>
  <object class="pygubu.builder.widgets.dialog" id="dialog" named="True">
    <property name="height">100</property>
    <property name="modal">true</property>
    <property name="resizable">both</property>
    <property name="title" translatable="yes">Save Changes</property>
    <property name="width">200</property>
    <bind sequence="&lt;&lt;DialogClose&gt;&gt;" handler="on_dialog_close" add="" />
    <child>
      <object class="ttk.Frame" id="fcontainer">
        <property name="height">200</property>
        <property name="padding">5p</property>
        <property name="width">200</property>
        <layout manager="pack">
          <property name="expand">true</property>
          <property name="fill">both</property>
          <property name="side">top</property>
        </layout>
        <containerlayout manager="grid">
          <property type="col" id="0" name="weight">1</property>
          <property type="row" id="0" name="weight">1</property>
        </containerlayout>
        <child>
          <object class="ttk.Frame" id="frame3">
            <property name="height">200</property>
            <property name="padding">10p 10p 10p 20p</property>
            <property name="width">200</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="row">0</property>
              <property name="sticky">nsew</property>
            </layout>
            <child>
              <object class="ttk.Label" id="labelicon">
                <property name="image">scd_msg</property>
                <property name="text" translatable="yes">SaveIcon</property>
                <layout manager="pack">
                  <property name="fill">y</property>
                  <property name="side">left</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Frame" id="frame5">
                <property name="height">200</property>
                <property name="padding">10 0 0 0</property>
                <property name="width">200</property>
                <layout manager="pack">
                  <property name="expand">true</property>
                  <property name="fill">x</property>
                  <property name="side">top</property>
                </layout>
                <child>
                  <object class="ttk.Label" id="lbl_message">
                    <property name="font">TkDefaultFont</property>
                    <property name="padding">0 0 0 5</property>
                    <property name="text" translatable="yes">Message</property>
                    <property name="textvariable">string:message_var</property>
                    <layout manager="pack">
                      <property name="fill">x</property>
                      <property name="side">top</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="ttk.Label" id="lbl_detail">
                    <property name="font">TkDefaultFont</property>
                    <property name="text" translatable="yes">Detail</property>
                    <property name="textvariable">string:detail_var</property>
                    <layout manager="pack">
                      <property name="fill">x</property>
                      <property name="side">top</property>
                    </layout>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="ttk.Frame" id="fbuttons">
            <property name="height">200</property>
            <property name="width">200</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="row">1</property>
              <property name="sticky">nsew</property>
            </layout>
            <containerlayout manager="grid">
              <property type="col" id="0" name="weight">1</property>
              <property type="col" id="1" name="weight">1</property>
              <property type="col" id="2" name="weight">1</property>
            </containerlayout>
            <child>
              <object class="ttk.Button" id="btn_cancel">
                <property name="command" type="command" cbtype="simple" args="">on_cancel</property>
                <property name="compound">left</property>
                <property name="image">scd_cancel</property>
                <property name="text" translatable="yes">Cancel</property>
                <layout manager="grid">
                  <property name="column">0</property>
                  <property name="row">0</property>
                  <property name="sticky">ew</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Button" id="btn_dontsave">
                <property name="command" type="command" cbtype="simple">on_discard</property>
                <property name="compound">left</property>
                <property name="image">scd_discard</property>
                <property name="text" translatable="yes">Discard</property>
                <layout manager="grid">
                  <property name="column">1</property>
                  <property name="padx">5p</property>
                  <property name="row">0</property>
                  <property name="sticky">ew</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Button" id="btn_save" named="True">
                <property name="command" type="command" cbtype="simple" args="">on_save</property>
                <property name="compound">left</property>
                <property name="image">scd_save</property>
                <property name="text" translatable="yes">Save</property>
                <layout manager="grid">
                  <property name="column">2</property>
                  <property name="row">0</property>
                  <property name="sticky">ew</property>
                </layout>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
