<?xml version='1.0' encoding='utf-8'?>
<interface version="1.3">
  <object class="pygubu.builder.widgets.dialog" id="preferences">
    <property name="geometry">580x410</property>
    <property name="height">200</property>
    <property name="minsize">480|360</property>
    <property name="modal">true</property>
    <property name="resizable">both</property>
    <property name="title" translatable="yes">Pygubu Preferences</property>
    <property name="width">200</property>
    <bind sequence="&lt;&lt;DialogClose&gt;&gt;" handler="on_dialog_close" add="" />
    <child>
      <object class="ttk.Frame" id="Frame_5">
        <property name="height">200</property>
        <property name="padding">5</property>
        <property name="width">200</property>
        <layout manager="pack">
          <property name="expand">true</property>
          <property name="fill">both</property>
          <property name="side">top</property>
        </layout>
        <child>
          <object class="ttk.Notebook" id="Notebook_1">
            <property name="height">200</property>
            <property name="width">200</property>
            <layout manager="pack">
              <property name="expand">true</property>
              <property name="fill">both</property>
              <property name="side">top</property>
            </layout>
            <child>
              <object class="ttk.Notebook.Tab" id="Tab_general">
                <property name="sticky">nsew</property>
                <property name="text" translatable="yes">General</property>
                <child>
                  <object class="ttk.Frame" id="fgeneral">
                    <property name="height">200</property>
                    <property name="padding">5</property>
                    <property name="width">200</property>
                    <layout manager="pack">
                      <property name="side">top</property>
                    </layout>
                    <containerlayout manager="grid">
                      <property type="col" id="0" name="weight">1</property>
                    </containerlayout>
                    <child>
                      <object class="ttk.Labelframe" id="general_frame">
                        <property name="height">200</property>
                        <property name="padding">10 0 10 5</property>
                        <property name="text" translatable="yes">General preferences</property>
                        <property name="width">200</property>
                        <layout manager="grid">
                          <property name="column">0</property>
                          <property name="pady">0 5</property>
                          <property name="row">0</property>
                          <property name="sticky">nsew</property>
                        </layout>
                        <containerlayout manager="grid">
                          <property type="col" id="1" name="weight">1</property>
                          <property type="row" id="0" name="pad">4</property>
                          <property type="row" id="1" name="pad">0</property>
                          <property type="row" id="2" name="pad">4</property>
                        </containerlayout>
                        <child>
                          <object class="ttk.Label" id="lblpt">
                            <property name="text" translatable="yes">Preferred widget set:</property>
                            <layout manager="grid">
                              <property name="sticky">ew</property>
                            </layout>
                          </object>
                        </child>
                        <child>
                          <object class="pygubu.builder.widgets.combobox" id="widget_set">
                            <property name="keyvariable">string:widget_set</property>
                            <layout manager="grid">
                              <property name="column">1</property>
                              <property name="pady">4</property>
                              <property name="row">0</property>
                              <property name="sticky">e</property>
                            </layout>
                          </object>
                        </child>
                        <child>
                          <object class="ttk.Label" id="label1">
                            <property name="text" translatable="yes">Preferred ttk theme:</property>
                            <layout manager="grid">
                              <property name="column">0</property>
                              <property name="row">1</property>
                              <property name="sticky">ew</property>
                            </layout>
                          </object>
                        </child>
                        <child>
                          <object class="pygubu.builder.widgets.combobox" id="cbox_ttk_theme">
                            <property name="keyvariable">string:ttk_theme</property>
                            <layout manager="grid">
                              <property name="column">1</property>
                              <property name="pady">4</property>
                              <property name="row">1</property>
                              <property name="sticky">e</property>
                            </layout>
                          </object>
                        </child>
                        <child>
                          <object class="ttk.Label" id="lbl_preferred_layout_manager">
                            <property name="text" translatable="yes">Preferred layout manager:</property>
                            <layout manager="grid">
                              <property name="column">0</property>
                              <property name="row">2</property>
                              <property name="sticky">ew</property>
                            </layout>
                          </object>
                        </child>
                        <child>
                          <object class="pygubu.builder.widgets.combobox" id="cbox_layout_manager">
                            <property name="keyvariable">string:default_layout_manager</property>
                            <layout manager="grid">
                              <property name="column">1</property>
                              <property name="pady">4</property>
                              <property name="row">2</property>
                              <property name="sticky">e</property>
                            </layout>
                          </object>
                        </child>
                        <child>
                          <object class="ttk.Checkbutton" id="chk_center_preview">
                            <property name="offvalue">no</property>
                            <property name="onvalue">yes</property>
                            <property name="text" translatable="yes">Center toplevel preview window</property>
                            <property name="variable">string:center_preview</property>
                            <layout manager="grid">
                              <property name="column">0</property>
                              <property name="pady">4</property>
                              <property name="row">3</property>
                              <property name="sticky">w</property>
                            </layout>
                          </object>
                        </child>
                      </object>
                    </child>
                    <child>
                      <object class="ttk.Labelframe" id="labelframe_1">
                        <property name="height">200</property>
                        <property name="padding">10 0 10 5</property>
                        <property name="text" translatable="yes">Widget naming</property>
                        <property name="width">200</property>
                        <layout manager="grid">
                          <property name="column">0</property>
                          <property name="padx">5 0</property>
                          <property name="row">1</property>
                          <property name="sticky">nsew</property>
                        </layout>
                        <containerlayout manager="grid">
                          <property type="col" id="0" name="weight">1</property>
                        </containerlayout>
                        <child>
                          <object class="ttk.Radiobutton" id="radiobutton_1">
                            <property name="text" translatable="yes">No separator (e.g. button1)</property>
                            <property name="value">NONE</property>
                            <property name="variable">string:widget_naming_separator</property>
                            <layout manager="grid">
                              <property name="column">0</property>
                              <property name="row">0</property>
                              <property name="sticky">w</property>
                            </layout>
                          </object>
                        </child>
                        <child>
                          <object class="ttk.Radiobutton" id="radiobutton_2">
                            <property name="text" translatable="yes">Use "_" as separator (e.g. button_1)</property>
                            <property name="value">UNDERSCORE</property>
                            <property name="variable">string:widget_naming_separator</property>
                            <layout manager="grid">
                              <property name="column">0</property>
                              <property name="row">1</property>
                              <property name="sticky">w</property>
                            </layout>
                          </object>
                        </child>
                        <child>
                          <object class="ttk.Checkbutton" id="checkbutton_1">
                            <property name="offvalue">no</property>
                            <property name="onvalue">yes</property>
                            <property name="text" translatable="yes">Uppercase first letter (e.g. Button…)</property>
                            <property name="variable">string:widget_naming_ufletter</property>
                            <layout manager="grid">
                              <property name="column">0</property>
                              <property name="pady">4</property>
                              <property name="row">2</property>
                              <property name="sticky">w</property>
                            </layout>
                          </object>
                        </child>
                        <child>
                          <object class="ttk.Label" id="label_1">
                            <property name="text" translatable="yes">This does not affect previously generated names.</property>
                            <layout manager="grid">
                              <property name="column">0</property>
                              <property name="pady">4</property>
                              <property name="row">3</property>
                              <property name="sticky">w</property>
                            </layout>
                          </object>
                        </child>
                      </object>
                    </child>
                    <child>
                      <object class="ttk.Labelframe" id="fhidden">
                        <property name="height">200</property>
                        <property name="padding">10 0 10 5</property>
                        <property name="text" translatable="yes">Hidden options</property>
                        <property name="width">200</property>
                        <layout manager="grid">
                          <property name="column">0</property>
                          <property name="row">2</property>
                          <property name="sticky">nsew</property>
                        </layout>
                        <containerlayout manager="grid">
                          <property type="col" id="1" name="weight">1</property>
                        </containerlayout>
                        <child>
                          <object class="ttk.Label" id="label_1_2">
                            <property name="text" translatable="yes">Window geometry:</property>
                            <layout manager="grid">
                              <property name="column">0</property>
                              <property name="row">0</property>
                            </layout>
                          </object>
                        </child>
                        <child>
                          <object class="ttk.Entry" id="entry_1">
                            <property name="state">readonly</property>
                            <property name="textvariable">string:geometry</property>
                            <layout manager="grid">
                              <property name="column">1</property>
                              <property name="row">0</property>
                            </layout>
                          </object>
                        </child>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="ttk.Notebook.Tab" id="Tab_cwidgets">
                <property name="sticky">nsew</property>
                <property name="text" translatable="yes">Custom Widgets</property>
                <child>
                  <object class="ttk.Frame" id="Frame_1">
                    <property name="height">200</property>
                    <property name="padding">5</property>
                    <property name="width">200</property>
                    <layout manager="pack">
                      <property name="expand">true</property>
                      <property name="fill">both</property>
                      <property name="side">top</property>
                    </layout>
                    <child>
                      <object class="ttk.Frame" id="Frame_2">
                        <property name="height">200</property>
                        <property name="width">200</property>
                        <layout manager="pack">
                          <property name="expand">true</property>
                          <property name="fill">both</property>
                          <property name="side">top</property>
                        </layout>
                        <containerlayout manager="grid">
                          <property type="col" id="0" name="weight">1</property>
                          <property type="row" id="0" name="weight">0</property>
                          <property type="row" id="1" name="weight">1</property>
                        </containerlayout>
                        <child>
                          <object class="ttk.Label" id="Label_1">
                            <property name="font">TkHeadingFont</property>
                            <property name="text" translatable="yes">Custom Widgets Builders</property>
                            <layout manager="grid">
                              <property name="column">0</property>
                              <property name="row">0</property>
                              <property name="sticky">w</property>
                            </layout>
                          </object>
                        </child>
                        <child>
                          <object class="pygubu.builder.widgets.scrollbarhelper" id="scrollbarhelper_1">
                            <property name="scrolltype">both</property>
                            <layout manager="grid">
                              <property name="column">0</property>
                              <property name="row">1</property>
                              <property name="sticky">nsew</property>
                            </layout>
                            <child>
                              <object class="ttk.Treeview" id="cwtv">
                                <property name="height">8</property>
                                <property name="selectmode">extended</property>
                                <property name="show">tree</property>
                                <layout manager="pack">
                                  <property name="expand">true</property>
                                  <property name="fill">both</property>
                                  <property name="side">top</property>
                                </layout>
                                <child>
                                  <object class="ttk.Treeview.Column" id="Column_1">
                                    <property name="column_anchor">w</property>
                                    <property name="heading_anchor">w</property>
                                    <property name="minwidth">20</property>
                                    <property name="stretch">true</property>
                                    <property name="text" translatable="yes">modules</property>
                                    <property name="tree_column">true</property>
                                    <property name="visible">true</property>
                                    <property name="width">250</property>
                                  </object>
                                </child>
                              </object>
                            </child>
                          </object>
                        </child>
                        <child>
                          <object class="ttk.Frame" id="Frame_3">
                            <property name="height">200</property>
                            <property name="padding">0 5</property>
                            <property name="width">200</property>
                            <layout manager="grid">
                              <property name="column">0</property>
                              <property name="row">2</property>
                              <property name="sticky">ew</property>
                            </layout>
                            <containerlayout manager="grid">
                              <property type="col" id="0" name="weight">0</property>
                              <property type="col" id="1" name="weight">1</property>
                              <property type="row" id="0" name="weight">0</property>
                              <property type="row" id="1" name="weight">1</property>
                            </containerlayout>
                            <child>
                              <object class="ttk.Label" id="Label_2">
                                <property name="padding">0 0 0 2</property>
                                <property name="text" translatable="yes">Requires restart.</property>
                                <layout manager="grid">
                                  <property name="column">0</property>
                                  <property name="row">0</property>
                                  <property name="sticky">w</property>
                                </layout>
                              </object>
                            </child>
                            <child>
                              <object class="ttk.Button" id="path_remove">
                                <property name="command" type="command" cbtype="simple" args="">on_pathremove_clicked</property>
                                <property name="state">disabled</property>
                                <property name="text" translatable="yes">-</property>
                                <property name="width">8</property>
                                <layout manager="grid">
                                  <property name="column">1</property>
                                  <property name="padx">0 5</property>
                                  <property name="row">0</property>
                                  <property name="sticky">e</property>
                                </layout>
                              </object>
                            </child>
                            <child>
                              <object class="ttk.Button" id="path_add">
                                <property name="command" type="command" cbtype="simple" args="">on_pathadd_clicked</property>
                                <property name="text" translatable="yes">+</property>
                                <property name="width">8</property>
                                <layout manager="grid">
                                  <property name="column">2</property>
                                  <property name="padx">5 0</property>
                                  <property name="row">0</property>
                                  <property name="sticky">e</property>
                                </layout>
                              </object>
                            </child>
                          </object>
                        </child>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="ttk.Notebook.Tab" id="Tab_ttk_styles">
                <property name="sticky">nsew</property>
                <property name="text" translatable="yes">Ttk Styles</property>
                <child>
                  <object class="ttk.Frame" id="frame_ttkstyles">
                    <property name="height">200</property>
                    <property name="padding">5</property>
                    <property name="width">200</property>
                    <layout manager="pack">
                      <property name="expand">true</property>
                      <property name="fill">both</property>
                      <property name="side">top</property>
                    </layout>
                    <child>
                      <object class="ttk.Frame" id="frame_title">
                        <property name="height">200</property>
                        <property name="width">200</property>
                        <layout manager="pack">
                          <property name="expand">false</property>
                          <property name="fill">x</property>
                          <property name="side">top</property>
                        </layout>
                        <containerlayout manager="grid">
                          <property type="col" id="0" name="weight">1</property>
                          <property type="row" id="0" name="weight">0</property>
                        </containerlayout>
                        <child>
                          <object class="ttk.Label" id="lbl_title">
                            <property name="font">TkHeadingFont</property>
                            <property name="text" translatable="yes">Ttk Styles</property>
                            <layout manager="grid">
                              <property name="column">0</property>
                              <property name="row">0</property>
                              <property name="sticky">w</property>
                            </layout>
                          </object>
                        </child>
                      </object>
                    </child>
                    <child>
                      <object class="ttk.Frame" id="frame_ttk_files">
                        <property name="height">200</property>
                        <property name="width">200</property>
                        <layout manager="pack">
                          <property name="expand">false</property>
                          <property name="fill">x</property>
                          <property name="pady">20</property>
                          <property name="side">top</property>
                        </layout>
                        <containerlayout manager="grid">
                          <property type="col" id="0" name="weight">1</property>
                        </containerlayout>
                        <child>
                          <object class="ttk.Label" id="lbl_definition_file">
                            <property name="text" translatable="yes">Style Definition File:</property>
                            <layout manager="grid">
                              <property name="column">0</property>
                              <property name="row">0</property>
                              <property name="sticky">w</property>
                            </layout>
                          </object>
                        </child>
                        <child>
                          <object class="ttk.Entry" id="entry_style_definition_file">
                            <property name="state">readonly</property>
                            <property name="textvariable">string:v_style_definition_file</property>
                            <layout manager="grid">
                              <property name="column">0</property>
                              <property name="columnspan">2</property>
                              <property name="row">1</property>
                              <property name="sticky">nsew</property>
                            </layout>
                          </object>
                        </child>
                        <child>
                          <object class="ttk.Frame" id="frame1">
                            <property name="height">200</property>
                            <property name="padding">0 5</property>
                            <property name="width">200</property>
                            <layout manager="grid">
                              <property name="column">0</property>
                              <property name="row">3</property>
                              <property name="sticky">ew</property>
                            </layout>
                            <child>
                              <object class="ttk.Button" id="btn_browse_definition_file">
                                <property name="command" type="command" cbtype="simple">on_clicked_select_style_file</property>
                                <property name="text" translatable="yes">Browse …</property>
                                <layout manager="pack">
                                  <property name="padx">0 10</property>
                                  <property name="side">left</property>
                                </layout>
                              </object>
                            </child>
                            <child>
                              <object class="ttk.Button" id="btn_remove_style_definition">
                                <property name="command" type="command" cbtype="simple">on_clicked_remove_style_file</property>
                                <property name="text" translatable="yes">Remove</property>
                                <layout manager="pack">
                                  <property name="padx">0 10</property>
                                  <property name="side">left</property>
                                </layout>
                              </object>
                            </child>
                            <child>
                              <object class="ttk.Button" id="btn_new_style_definition">
                                <property name="command" type="command" cbtype="simple">on_create_new_definition_clicked</property>
                                <property name="text" translatable="yes">Create New …</property>
                                <layout manager="pack">
                                  <property name="side">left</property>
                                </layout>
                              </object>
                            </child>
                          </object>
                        </child>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="ttk.Frame" id="Frame_4">
            <property name="height">200</property>
            <property name="padding">0 5 0 0</property>
            <property name="width">200</property>
            <layout manager="pack">
              <property name="side">right</property>
            </layout>
            <child>
              <object class="ttk.Button" id="cancel">
                <property name="command" type="command" cbtype="simple" args="">on_cancel</property>
                <property name="text" translatable="yes">Cancel</property>
                <layout manager="grid">
                  <property name="column">0</property>
                  <property name="padx">0 5</property>
                  <property name="row">0</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Button" id="dialog_close">
                <property name="command" type="command" cbtype="simple" args="">on_dialog_close</property>
                <property name="default">active</property>
                <property name="text" translatable="yes">Ok</property>
                <layout manager="grid">
                  <property name="column">1</property>
                  <property name="padx">5 0</property>
                  <property name="row">0</property>
                </layout>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
